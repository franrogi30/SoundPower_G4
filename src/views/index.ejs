      <div class="container-fluid carrousel">
        <div class="row">
          <div
            id="carouselExampleControls"
            class="carousel slide col-12"
            data-ride="carousel"
          >
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img
                  class="d-block w-100"
                  src="/images/bandasrojo.jpg"
                  alt="First slide"
                />
              </div>
              <div class="carousel-item">
                <img
                  class="d-block w-100"
